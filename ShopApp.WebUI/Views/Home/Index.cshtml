@model ProductListModel

    <div class="row">
        <div class="col-md-3">
            @await Component.InvokeAsync("CategoryList")
        </div>

        <div class="col-md-9">
            @await Html.PartialAsync("_PartialSlider")

            <div class="row">
                @foreach (var item in Model.Products)
                {
                   
                    @await Html.PartialAsync("_PartialProduct",item)
                }

            </div>
        </div>
    </div>

